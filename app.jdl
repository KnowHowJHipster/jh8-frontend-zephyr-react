application {
  config {
    baseName Boutique
    packageName org.iqkv.boutique
    authenticationType jwt
    prodDatabaseType postgresql
    buildTool maven
    clientFramework react
    clientTheme zephyr
    enableTranslation true
    nativeLanguage en
    languages [en, ru, fr, it]
    skipClient false
    skipServer true
    testFrameworks  [cypress, cucumber]
    skipUserManagement true
  }
}